{% extends "_layouts/blank-not-logged.html.twig" %}

{% block title %}
    {{ 'spaces'|trans }}
{% endblock %}

{% block content %}
    <section class="white-square">
        {% include 'admin/space/_partials/breadcrumb.html.twig' %}

        {% include 'admin/space/_partials/title.html.twig' %}

        {% include "_components/entity-dashboard.html.twig" with { 'dashboard': dashboard} %}

        {% include 'admin/space/_partials/tabs/tabs.html.twig' %}
    </section>

    {% include 'admin/space/_partials/sort-dropdown.html.twig' %}

    <div class="filter-and-spaces-container mt-4">
        <div class="spaces-list">
            {% for space in spaces %}
                {% include 'admin/space/_partials/space-card.html.twig' with { space: space } %}
            {% endfor %}
        </div>
    </div>
{% endblock %}
